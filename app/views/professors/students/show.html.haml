.page-header
  = link_to professors_students_path, class: 'btn btn-default' do
    %span.glyphicon.glyphicon-list-alt
    Back
  &nbsp;
  = link_to edit_professors_student_path(@professors_student), class: 'btn btn-primary' do
    %span.glyphicon.glyphicon-pencil
    Edit
  %h1= t(:show_student)

%dl.dl-horizontal
  %dt Name:
  %dd= @professors_student.name
  %br
  %dt Student:
  %dd= @professors_student.student_id
  %br
  %dt Email:
  %dd= @professors_student.email
