.page-header
  %h1=t(:listing_attempts)

.table-responsive
  %table.table.table-striped.table-bordered.table-hover
    %thead
      %tr
        %th= t(:quizzes)
        %th

    %tbody
      - @active_quizzes.each do |quiz|
        %tr
          %td= quiz.id
          %td= link_to t(:take_quiz), new_attempt_path(quiz_id: quiz.id)
